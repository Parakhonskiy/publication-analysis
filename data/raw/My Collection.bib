issn = {03043894},
journal = {Journal of Hazardous Materials},
keywords = {Label induced alterations,Labelling,Leaching,Microplastic detection,Nanoplastics},
month = {jan},
number = {July 2023},
pages = {132785},
pmid = {37856963},
publisher = {Elsevier B.V.},
title = {{Labelling of micro- and nanoplastics for environmental studies: state-of-the-art and future challenges}},
url = {https://doi.org/10.1016/j.jhazmat.2023.132785 https://linkinghub.elsevier.com/retrieve/pii/S0304389423020691},
volume = {462},
