issn = {02728842},
journal = {Ceramics International},
keywords = {Calcium carbonate,Machine learning,Model prediction,Phase transition,Surface morphol,Surface morphology},
month = {jul},
number = {13},
pages = {23284--23295},
publisher = {Elsevier Ltd},
title = {{Machine learning insights into CaCO3 phase transitions: Synthesis and phase prediction}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0272884224014329 https://doi.org/10.1016/j.ceramint.2024.04.052},
