issn = {02786915},
journal = {Food and Chemical Toxicology},
keywords = {Anti-cancer,Caspase independent apoptosis,Cell blebbing,Cell death,Ribosome-inactivating protein},
month = {nov},
number = {July},
pages = {112590},
pmid = {34601042},
title = {{The type-1 ribosome-inactivating protein OsRIP1 triggers caspase-independent apoptotic-like death in HeLa cells}},
