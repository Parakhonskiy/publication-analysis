file = {:C$\backslash$:/Users/lotus/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/German et al. - 2018 - High-efficiency freezing-induced loading of inorganic nanoparticles and proteins into micron- and submicron-sized.pdf:pdf},
issn = {2045-2322},
journal = {Scientific Reports},
month = {dec},
number = {1},
pages = {17763},
title = {{High-efficiency freezing-induced loading of inorganic nanoparticles and proteins into micron- and submicron-sized porous particles}},
url = {http://www.nature.com/articles/s41598-018-35846-x https://www.nature.com/articles/s41598-018-35846-x},
volume = {8},
