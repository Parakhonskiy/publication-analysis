issn = {1616-5187},
journal = {Macromolecular Bioscience},
month = {jul},
number = {7},
pages = {2000081},
title = {{Temperature Window for Encapsulation of an Enzyme into Thermally Shrunk, CaCO 3 Templated Polyelectrolyte Multilayer Capsules}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mabi.202000081 https://onlinelibrary.wiley.com/doi/10.1002/mabi.202000081},
