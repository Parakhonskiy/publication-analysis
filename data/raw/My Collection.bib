issn = {2296-2646},
journal = {Frontiers in Chemistry},
keywords = {colloids,flat surfaces,hybrid materials,inorganics,modifications and applications,nanoparticles,organics},
month = {jan},
number = {January},
pages = {1--23},
title = {{Hierarchy of hybrid materials. Part-II: The place of organics-on-inorganics in it, their composition and applications}},
