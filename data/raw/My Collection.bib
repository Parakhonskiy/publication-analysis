@article{Bukreeva2005,
abstract = {Langmuir-Blodgett films with high X-ray contrast in between the neighboring bilayers are prepared using two-chamber Langmuir trough. These films consist of alternating bilayers of lead stearate and nickel stearate, lead stearate and copper stearate. Prepared samples do not contain free stearic acid as confirmed by Fourier transform infrared spectroscopy and surface pressure isotherms. The superlattice structure with the unit cell size equaled to the sum of thicknesses of two bilayers in the direction perpendicular to the substrate was determined by X-ray analysis. It is shown that surface roughness is relatively small and the organization of bi- and tetra-layers is rather perfect.},
author = {Bukreeva, T. V. and Dembo, K. A. and Myagkov, I. V. and Parakhonsky, B. V. and Plotnikov, G. S. and Feigin, L. A.},
issn = {02073528},
journal = {Poverkhnost Rentgenovskie Sinkhronnye i Nejtronnye Issledovaniya},
number = {7},
pages = {23--29},
title = {{Formation of Langmuir-Blodgett superlattices consisting of bilayers of different metal stearates}},
year = {2005}
}
