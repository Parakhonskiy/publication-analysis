pages = {1--17},
publisher = {CIMNE},
title = {{Combining Wood Protection Options to Enhance Resistance against Decay and Improve Fire Safety of Engineered Wood Products like CLT}},
url = {https://www.scipedia.com/public/Van{\_}Acker{\_}et{\_}al{\_}2023a},
year = {2023}
}
@article{Abalymov2023,
